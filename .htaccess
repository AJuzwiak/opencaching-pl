RewriteEngine on

RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [NE,R=301,L]

RewriteBase /

RewriteRule ^search\.(gpx|kml|loc|ov2|ovl|txt|wpt|uam|xml|zip)$ search.php?%{QUERY_STRING} [NC]
RewriteRule ^ocpl(\d+)\.(gpx|kml|loc|ov2|ovl|txt|wpt|uam|xml|zip)$ search.php?queryid=$1&output=$2&%{QUERY_STRING} [NC]
RewriteRule ^ocplgpx(\d+)\.(gpx)$ search.php?queryid=$1&output=gpxgc&%{QUERY_STRING} [NC]
RewriteRule ^/wiki(.*)$ http://wiki.opencaching.pl$1 [R,L]
RewriteRule ^/statpics/([0-9]+)\.jpg$ ocstats.php?userid=$1 [R,L]

RewriteRule ^(OP[0-9A-F][0-9A-F][0-9A-F][0-9A-F])$ viewcache.php?wp=$1 [L]

FileETag None
